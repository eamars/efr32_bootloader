<?xml version="1.0"?>
<cli>
  <group id="plugin-rf4ce-mso-ir-rf-database-recipient" name="Plugin Commands: RF4CE Multiple System Operators IR-RF Database Recipient">
    <description>
      The RF4CE MSO IR-RF Database Recipient plugin contributes several commands to the application framework's CLI.
    </description>
  </group>
  <command cli="plugin rf4ce-mso-ir-rf-database-recipient add-flags" functionName="emberAfRf4ceMsoIrRfDatabaseRecipientAddFlagsCommand" group="plugin-rf4ce-mso-ir-rf-database-recipient">
    <description>
      Add the flags of the IR-RF database element.
    </description>
    <arg name="flags" type="INT8U" description="The flags." />
  </command>
  <command cli="plugin rf4ce-mso-ir-rf-database-recipient add-pressed" functionName="emberAfRf4ceMsoIrRfDatabaseRecipientAddPressedCommand" group="plugin-rf4ce-mso-ir-rf-database-recipient">
    <description>
      Add the RF pressed descriptor of the IR-RF database element.
    </description>
    <arg name="rfConfig"  type="INT8U"        description="The RF config." />
    <arg name="txOptions" type="INT8U"        description="The TX options." />
    <arg name="payload"   type="OCTET_STRING" description="The payload." />
  </command>
  <command cli="plugin rf4ce-mso-ir-rf-database-recipient add-repeated" functionName="emberAfRf4ceMsoIrRfDatabaseRecipientAddRepeatedCommand" group="plugin-rf4ce-mso-ir-rf-database-recipient">
    <description>
      Add the RF repeated descriptor of the IR-RF database element.
    </description>
    <arg name="rfConfig"  type="INT8U"        description="The RF config." />
    <arg name="txOptions" type="INT8U"        description="The TX options." />
    <arg name="payload"   type="OCTET_STRING" description="The payload." />
  </command>
  <command cli="plugin rf4ce-mso-ir-rf-database-recipient add-released" functionName="emberAfRf4ceMsoIrRfDatabaseRecipientAddReleasedCommand" group="plugin-rf4ce-mso-ir-rf-database-recipient">
    <description>
      Add the RF released descriptor of the IR-RF database element.
    </description>
    <arg name="rfConfig"  type="INT8U"        description="The RF config." />
    <arg name="txOptions" type="INT8U"        description="The TX options." />
    <arg name="payload"   type="OCTET_STRING" description="The payload." />
  </command>
  <command cli="plugin rf4ce-mso-ir-rf-database-recipient add-ir" functionName="emberAfRf4ceMsoIrRfDatabaseRecipientAddIrCommand" group="plugin-rf4ce-mso-ir-rf-database-recipient">
    <description>
      Add the IR descriptor of the IR-RF database element.
    </description>
    <arg name="irConfig" type="INT8U"        description="The IR config." />
    <arg name="irCode"   type="OCTET_STRING" description="The IR code." />
  </command>
  <command cli="plugin rf4ce-mso-ir-rf-database-recipient add" functionName="emberAfRf4ceMsoIrRfDatabaseRecipientAddCommand" group="plugin-rf4ce-mso-ir-rf-database-recipient">
    <description>
      Add the IR-RF database element.
    </description>
    <arg name="keyCode" type="INT8U" description="The key code." />
  </command>
  <command cli="plugin rf4ce-mso-ir-rf-database-recipient remove" functionName="emberAfRf4ceMsoIrRfDatabaseRecipientRemoveCommand" group="plugin-rf4ce-mso-ir-rf-database-recipient">
    <description>
      Remove the IR-RF database element.
    </description>
    <arg name="keyCode" type="INT8U" description="The key code." />
  </command>
  <command cli="plugin rf4ce-mso-ir-rf-database-recipient remove-all" functionName="emberAfRf4ceMsoIrRfDatabaseRecipientRemoveAllCommand" group="plugin-rf4ce-mso-ir-rf-database-recipient">
    <description>
      Remove all IR-RF database elements.
    </description>
  </command>
  <command cli="plugin rf4ce-mso-ir-rf-database-recipient get-callback" functionName="emberAfRf4ceMsoIrRfDatabaseRecipientGetCallbackCommand" group="plugin-rf4ce-mso-ir-rf-database-recipient">
    <description>
      Call the Get Callback and print out IR-RF database element.
    </description>
    <arg name="keyCode" type="INT8U" description="The key code." />
  </command>
</cli>
